   pwdauth:'',\n        }\n    },\n  \n    methods: {\n        async handleSubmitAuth() {\n            const newLocal=passwordHash.generate(this.pwdauth)\n            var hashedPassword = newLocal;\n            const response = await axios.post('http://127.0.0.1:5000/api/account',  {\n                usrnameauth: this.usrnameauth,\n                pwdauth: hashedPassword\n            });\n            if(response.data.status == '201'){\n                this.$router.push('/login') \n            }else{\n                this.message = response.data.message\n            }\n            \n        }\n    }\n    \n}\n</script>\n\n<style scoped>\n    .container {\n        padding: 2px 16px;\n        width: 30%;\n    }\n    input {\n        width: 10%;\n    }\n</style>"]}]}