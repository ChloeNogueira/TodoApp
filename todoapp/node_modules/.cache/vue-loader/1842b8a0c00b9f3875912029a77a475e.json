    \n\n        </form>\n     </div>\n    </div>\n\n</template>\n\n<script>\nimport axios from \"axios\"\n\nexport default {\n    name: 'Register',\n    data() {\n        return {\n            message:'' ,\n            usrnameauth:'',\n            pwdauth:'',\n        }\n    },\n  \n    methods: {\n        hashPassword(pwd) {\n            let hash = 0;\n            if(pwd === 0) return hash;\n            for (let i = 0; i< pwd.length; i++) {\n                let char = pwd.charCodeAt(i);\n                hash = ((hash<<5)-hash)+char;\n                hash = hash & hash; //Convert to 32bit integer\n            }\n            return hash;\n        },\n        async handleSubmitAuth() {\n            const hash = this.hashPassword(this.pwdauth)\n            const response = await axios.post('http://127.0.0.1:5000/api/account',  {\n                usrnameauth: this.usrnameauth,\n                pwdauth: hash\n            });\n            if(response.data.status == '201') {\n                this.$router.push('/login')\n            }else {\n                this.message = response.data.message\n            }        \n        }\n    }\n    \n}\n</script>\n\n<style scoped>\n    .container {\n        padding: 2px 16px;\n        width: 30%;\n    }\n    input {\n        width: 10%;\n    }\n</style>"]}]}