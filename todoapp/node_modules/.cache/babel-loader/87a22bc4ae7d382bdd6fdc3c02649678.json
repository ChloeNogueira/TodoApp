<script>\n\nexport default {\n    name: \"AddTodo\",\n    data() {\n        return {\n            name:'',\n            created_on:''\n        }\n    },\n    methods: {\n        addTodo(e) {\n            e.preventDefault();\n            const newTodo = {\n                name: this.name,\n                created_on:  this.created_on\n            }\n            // On l'envoie au parent\n            this.$emit('add-todo',newTodo);\n\n            // Efface dans la barre de saisie une fois ajouté\n            this.name = '';\n            this.created_on = '';\n        }\n    }\n}\n</script>\n\n<style scoped>\n    \n</style>\n"],"sourceRoot":"src/components"}]}